{
  "name": "opentype-svg-loader",
  "version": "0.3.2",
  "description": "A loader for webpack that lets you import TrueType and OpenType fonts converted to a SVG string.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"!(node_modules)/**/!(main.bundle).js\" \"*.js\"",
    "precommit": "npm run prettier && npm run test",
    "release": "np",
    "build-examples": "npm run build-example:json && npm run build-example:mustache",
    "build-example:json": "webpack --config examples/json/webpack.config.js",
    "build-example:mustache": "webpack --config examples/mustache/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schapka/opentype-svg-loader.git"
  },
  "keywords": [],
  "author": "Ren√© Schapka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schapka/opentype-svg-loader/issues"
  },
  "homepage": "https://github.com/schapka/opentype-svg-loader#readme",
  "devDependencies": {
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "np": "^2.13.1",
    "prettier": "^0.22.0",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "loader-utils": "^1.1.0",
    "mustache": "^2.3.0",
    "opentype.js": "^0.6.9",
    "shortid": "^2.2.8",
    "webpack-merge": "^4.1.0"
  }
}
